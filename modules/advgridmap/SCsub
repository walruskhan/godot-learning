#!/usr/bin/env python
from misc.utility.scons_hints import *

Import("env")
Import("env_modules")

env_advgridmap = env_modules.Clone()

# Godot source files
env_advgridmap.add_source_files(env.modules_sources, "*.cpp")
if env.editor_build:
    env_advgridmap.add_source_files(env.modules_sources, "editor/*.cpp")
